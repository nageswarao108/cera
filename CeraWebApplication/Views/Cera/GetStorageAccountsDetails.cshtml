@model List<CeraWebApplication.Models.CeraStorageAccount>
<h1>StorageAccounts List</h1>
@if (Model != null)
{
<table class="table table-striped table-hover" style="margin-top:1%">
    <thead>
        <tr>
            <th>
                Name
            </th>


            <th>
                ResourceGroupName
            </th>

            <th>
                RegionName
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ResourceGroupName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.RegionName)
    </td>
</tr>}
    </tbody>
</table>}
