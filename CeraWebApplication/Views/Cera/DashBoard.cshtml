@model List<CeraWebApplication.Models.ResourceTypeCount>
<style>
    .my-custom-scrollbar {
        position: relative;
        height: 280px;
        overflow: auto;
    }

    .table-wrapper-scroll-y {
        display: block;
    }
</style>
<h2>Dashboard</h2>
<hr />
<div>
    <div class="row">
        <div class="col-md-6">
            <h4>Resources Overview</h4>
            <div class="table-wrapper-scroll-y my-custom-scrollbar">
                <table class="table table-striped table-hover" style="margin-top:1%">
                    <thead>
                        <tr>
                            <th>
                                Resources
                            </th>
                            <th>
                                Count
                            </th>
                            <th>
                                Health
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in ViewBag.count)
                        {
                            <tr>
                                <td>
                                    @item.resourceType
                                </td>
                                <td>
                                    @item.count
                                </td>
                                <td>
                                    <a href="Cera/GetResourceHealth">
                                        Available
                                    </a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-md-6">
            <h4>Resource Spend</h4>
            <iframe width="500" height="300" src="https://app.powerbi.com/view?r=eyJrIjoiOTY5MmE1NTgtOTcxMC00MTgzLWI3MTEtNGI3MTg2YWRmNjVhIiwidCI6IjBlYWRiNzdlLTQyZGMtNDdmOC1iYmUzLWVjMjM5NWUwNzEyYyIsImMiOjZ9&pageName=ReportSection" frameborder="0" allowFullScreen="true"></iframe>
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-md-6">
            <h4>Resource Availability Map</h4>
            <iframe width="500" height="300" src="https://app.powerbi.com/view?r=eyJrIjoiMmE3OWFhZDYtYjk4Ny00NDE1LWI5ZjUtNTJhYTI0MmIxNDNkIiwidCI6IjBlYWRiNzdlLTQyZGMtNDdmOC1iYmUzLWVjMjM5NWUwNzEyYyIsImMiOjZ9" frameborder="0" allowFullScreen="true"></iframe>
        </div>
        <div class="col-md-6">
          
            <h4>Resources with Tags</h4>
            <div id="Tags" class="group">
                <iframe width="500" height="300" src="https://app.powerbi.com/view?r=eyJrIjoiYWExYjczYmYtNWMyNC00NWM1LWFlZTYtYjczMWVjOThjNzBlIiwidCI6IjBlYWRiNzdlLTQyZGMtNDdmOC1iYmUzLWVjMjM5NWUwNzEyYyIsImMiOjZ9" frameborder="0" allowFullScreen="true"></iframe>
            </div>
            
        </div>
    </div>
</div>
